{
  "name": "nx-angular-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "prepare": "npx husky install",
    "lint-prettier": "prettier --check .",
    "format": "prettier --write .",
    "pre-commit": "npm run lint && npm run test && lint-staged",
    "commit-msg": "npx --no-install commitlint --edit",
    "commit:lint": "commitlint -E HUSKY_GIT_PARAMS",
    "lint": "nx lint"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.0.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/schematics": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@nrwl/angular": "13.3.1",
    "cerializr": "^3.1.4",
    "lodash": "^4.17.21",
    "memo-decorator": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "eslint-plugin-unused-imports": "^2.0.0",
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nrwl/cli": "13.3.1",
    "@nrwl/cypress": "13.3.1",
    "@nrwl/eslint-plugin-nx": "13.3.1",
    "@nrwl/jest": "13.3.1",
    "@nrwl/linter": "13.3.1",
    "@nrwl/tao": "13.3.1",
    "@nrwl/workspace": "13.3.1",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "cypress": "^8.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "lint-staged": "^12.1.3",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
